			<section>
<% if @unapproved.any? %>
				<h2>Unapproved Users</h2>
				<div class="list">
	<% @unapproved.each do |user| %>
<%= render 'listing', user: user %>
	<% end %>
				</div>
<% end %>

				<h2>Users</h2>
				<div class="list">
<% @users.each do |user| %>
<%= render 'listing', user: user %>
<% end %>
				</div>
			</section>